{
    "description": "学习selenium测试技术",
    "manifest_version": 2,
    "name": "selenium auto test",
    "version": "1.0.0",
    "homepage_url": "https://github.com/y2ghost",
    "icons": {
        "16": "icons/icon16.png",
        "48": "icons/icon48.png",
        "128": "icons/icon128.png"
    },
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "run_at": "document_idle",
            "js": [
                "webext.js"
            ]
        }
    ]
}